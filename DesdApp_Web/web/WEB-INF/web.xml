<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Controlador</servlet-name>
        <servlet-class>controlador.Controlador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Inmueble</servlet-name>
        <servlet-class>controlador.Inmueble</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InicioClientesControlador</servlet-name>
        <servlet-class>controlador.InicioClientesControlador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorLogin</servlet-name>
        <servlet-class>controlador.ControladorLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorIndex</servlet-name>
        <servlet-class>controlador.ControladorIndex</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Inmueble</servlet-name>
        <url-pattern>/Inmueble</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InicioClientesControlador</servlet-name>
        <url-pattern>/InicioClientesControlador</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorLogin</servlet-name>
        <url-pattern>/ControladorLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Controlador</servlet-name>
        <url-pattern>/Controlador</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorIndex</servlet-name>
        <url-pattern>/ControladorIndex</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
